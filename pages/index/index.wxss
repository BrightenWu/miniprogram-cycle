/**index.wxss**/
.home_container {
  background-image: url("http://120.78.139.131:12121/Picture/home.jpg");
  align-items: center;
	justify-content: center;
}

.home_img {
  width: 100%;
  height: 100%;
}

.home_btn {
  font-size: 50rpx;
  border-radius: 93rpx;
  border: 0rpx;
  align-items: center;
	justify-content: center;
  display: flex;
  padding: 0;
  background-color: var(--red);
  color: var(--white);
  box-shadow: 5rpx 5rpx 5rpx #888888;
}

.home_btn.button-hover {
  transform: translate(1rpx, 1rpx);
}